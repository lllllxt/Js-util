{
    "name": "util-es",
    "version": "0.1.9",
    "description": "js工具集",
    "main": "dist/index.js",
    "scripts": {
        "start": "npm run build && npm run browserify && npm run uglifyjs",
        "build": "babel src --out-dir dist --presets env",
