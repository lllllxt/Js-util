    "bugs": {
        "url": "https://github.com/lllllxt/util-es/issues"
    },
    "homepage": "https://github.com/lllllxt/util-es#readme",
    "dependencies": {},
    "devDependencies": {
        "babel-cli": "^6.26.0",
        "babel-preset-env": "^1.7.0",
        "browserify": "^16.2.3",
        "uglify-js": "^3.4.9"
    }
}